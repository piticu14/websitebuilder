    <!-- Start of KEditor scripts -->

    <script type="text/javascript" src="{$basePath}/vendor/keditor/dist/js/keditor-1.1.5.js"></script>
    <script type="text/javascript" src="{$basePath}/vendor/keditor/dist/js/keditor-components-1.1.5.js"></script>
    <!-- End of KEditor scripts -->


    <script>
        $(function () {
            $.nette.init();

        });
    </script>
    <script src="{$basePath}/js/custom.js"></script>
    <script src="{$basePath}/js/nav_editor.js"></script>
    <script src="{$basePath}/js/logo_editor.js"></script>
    <script src="{$basePath}/js/social_media_editor.js"></script>

*}
{/block}


