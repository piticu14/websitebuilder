    <script>
        CKEDITOR.dtd.$removeEmpty['a'] = false;
        $.each(CKEDITOR.dtd.$removeEmpty, function (i, value) {
            CKEDITOR.dtd.$removeEmpty[i] = false;
        });

    </script>
